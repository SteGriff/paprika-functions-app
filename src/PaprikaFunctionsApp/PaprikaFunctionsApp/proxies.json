{
    "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "paprikaweb": {
      "matchCondition": {
        "route": "/"
      },
      "backendUri": "https://paprika-web.azurewebsites.net"
    },
    "paprikawebresources": {
      "matchCondition": {
        "route": "/res/{*file}"
      },
      "backendUri": "https://paprika-web.azurewebsites.net/res/{file}"
    }
  }
}